{
  "name": "ubi-ltr-infrastructure",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for UBI to LTR pipeline",
  "main": "bin/app.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy --all",
    "deploy:opensearch": "cdk deploy UbiLtr-OpenSearchStack",
    "deploy:storage": "cdk deploy UbiLtr-StorageStack",
    "deploy:processing": "cdk deploy UbiLtr-ProcessingStack",
    "destroy": "cdk destroy --all",
    "diff": "cdk diff",
    "bootstrap": "cdk bootstrap"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "aws-cdk": "^2.170.0",
    "typescript": "~5.3.0"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.170.0",
    "constructs": "^10.0.0"
  }
}
